<Window x:Class="Playroom_Kiosk.EndAdmissionForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Playroom_Kiosk"
        mc:Ignorable="d"
        Title="Cobrar Salida" SizeToContent="WidthAndHeight">
    <Grid Margin="10">
        <StackPanel>
            <Label FontSize="15">Cobrar Salida</Label>
            <Label>Número de entrada a cobrar</Label>
            <TextBox Height="30" Margin="0,0,0,10" x:Name="hangerTextBox" />
            <StackPanel Orientation="Horizontal" Grid.Row="2">
                <Button x:Name="btnAccept" Width="100" Height="40" Margin="5" Click="ButtonAccept_Click">Aceptar</Button>
                <Button x:Name="btnCancel" Width="100" Height="40" Margin="5" Click="ButtonCancel_Click">Cancelar</Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
